                              <TextField fx:id="addMarkerY" promptText="Y Coordinate">
                                 <VBox.margin>
                                    <Insets top="2.0" />
                                 </VBox.margin>
                              </TextField>
                              <TextField fx:id="addMarkerName" promptText="Name">
                                 <VBox.margin>
                                    <Insets top="2.0" />
                                 </VBox.margin>
                              </TextField>
                              <Label text="Region:" />
